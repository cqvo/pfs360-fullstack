		"vite": "^5.0.3",
		"vitest": "^2.0.4"
	},
	"dependencies": {
		"@auth/sveltekit": "^1.7.4",
		"@sveltejs/kit": "^2.8.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"bcrypt": "^5.1.1",
		"csv-parse": "^5.5.6",
		"lucide-svelte": "^0.460.1",
